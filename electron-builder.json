{
  "publish": false,
  "appId": "com.zhangqinghua.app",
  "productName": "unkonw_app",
  "directories": {
    "output": "release"
  },
  "files": [
    "dist/**/*",
    "package.json"
  ],
  "win": {
    "icon": "assets/icon.png",
    "artifactName": "install.exe",
    "target": {
      "target": "nsis",
      "arch": [
        "x64"
      ]
    }
  },
  "mac": {
    "icon": "assets/icon.png",
    "artifactName": "install.dmg",
    "target": [
      "dmg"
    ]
  },
  "nsis": {
    "oneClick": false,
    "perMachine": true,
    "shortcutName": "${productName}",
    "uninstallDisplayName": "${productName}",
    "deleteAppDataOnUninstall": true,
    "allowToChangeInstallationDirectory": true,
    "guid": "92b9443f-2a16-443f-9b53-6e21cba302e0"
  },
  "extraMetadata": {
    "HOME_PAGE": "unkonw_app"
  },
  "extraResources": [
    {
      "from": "electron-builder.json",
      "to": "config.json"
    }
  ]
}